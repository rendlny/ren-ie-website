{
  "name": "rendlny/spoopy-floofer-website",
  "type": "project",
  "require": {
      "phpmailer/phpmailer": "~6.1",
      "slim/slim": "^4.2",
      "slim/psr7": "^0.5.0",
      "tinymce/tinymce": "^5.0",
    "laravel/framework": "6.20.26",
    "php": "^7.3",
    "laravel/installer": "^3.2"
  },
  "require-dev": {
    "robmorgan/phinx": "^0.11.1",
    "phpstan/phpstan": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "Controllers\\": "web/Controllers/",
      "Models\\": "web/Models/"
    }
  },
  "scripts": {
    "test": [
      "vendor/bin/phpstan analyse -c phpstan.neon"
    ],
    "phinx": [
      "vendor/bin/phinx"
    ],
    "docker image": [
      "docker build ."
    ]
  }
}
